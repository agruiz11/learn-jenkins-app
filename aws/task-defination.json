{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "JenkinsApp-TaskDef",
    "containerDefinitions": [
        {
            "name": "jenkinsapp",
            "image": "000168738829.dkr.ecr.us-east-2.amazonaws.com/my-jenkins-app:#APP_VERSION#",
            "portMappings": [
                {
                    "name": "nginx-80-tpc",
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::000168738829:role/ecsTaskExecutionRole"
}